{
  "name": "contribution",
  "description":
    "GitHub contribution count & streak fetcher with zero dependencies",
  "version": "1.0.12",
  "author": "Jamie Straw <me@jamiestraw.com>",
  "bugs": {
    "url": "https://github.com/jamiestraw/contribution/issues"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1"
  },
  "homepage": "https://github.com/jamiestraw/contribution#readme",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "contribution",
    "contribution-graph",
    "fetcher",
    "github",
    "no-dependencies",
    "stat",
    "statistics",
    "streak"
  ],
  "license": "MIT",
  "main": "src/contribution.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiestraw/contribution.git"
  },
  "scripts": {
    "coverage": "codecov",
    "lint": "npm run lint:js",
    "lint:js": "eslint .",
    "format": "npm run format:js",
    "format:js": "prettier --write --single-quote 'src/*.js'",
    "test": "jest"
  }
}
